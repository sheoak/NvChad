{
 "coc.preferences.formatOnType": true,
  "coc.preferences.formatOnSaveFiletypes": ["javascript", "vue", "typescript"],
  "diagnostic.checkCurrentLine": true,
  "prettier.enable": true,
  "prettier.disableLanguages": [],
  "diagnostic.errorSign": "×",
  "diagnostic.infoSign": "",
  "diagnostic.messageTarget": "float",
  "diagnostic.refreshOnInsertMode": true,
  "diagnostic.warningSign": "×",
  "diagnostic.signPriority": 10,
  "diagnostic.hintSign": ">>",
  "diagnostic.virtualText": false,
  "diagnostic.enableSign": true,
  "emmet.priority": 1,
  "suggest.languageSourcePriority": 3,
  "emmet.showExpandedAbbreviation": "always",
  "emmet.showAbbreviationSuggestions": true,
  "emmet.includeLanguages": { "vue-html": "html", "vue-scss": "css", "vue": "css" },
  "list.insertMappings": {
    "<C-x>": "action:split",
    "<C-v>": "action:vsplit",
    "<C-n>": "normal:s",
    "<C-p>": "normal:r",
    "<C-s>": "normal:s",
    "<C-r>": "normal:r"
  },
  "list.normalMappings": {
    "s": "action:split",
    "v": "action:vsplit"
  },
  "eslint.autoFixOnSave": true,
  "eslint.packageManager": "yarn",
  "eslint.probe": ["javascript", "vue", "html", "typescript"],
  "eslint.debug": false,
  "eslint.format.enable": true,
  "python.jediEnabled": false,
  "python.linting.flake8Enabled": true,
  "python.linting.enabled": true,
  "python.analysis.diagnosticEnabled": true,
  "python.formatting.provider": "black",
  "python.venvFolders": [".virtualenvs"],
  "snippets.userSnippetsDirectory": "~/.config/nvim/snippets",
  "suggest.noselect": true,
  "suggest.enablePreselect": true,
  "suggest.maxCompleteItemCount": 20,
  "volar-tools.enable": true,
  "volar.preview.port": 8000,
  "volar.enable": true,
  "volar.progressOnInitialization.enable": false,
  "volar.autoCreateQuotes": true,
  "volar.diagnostics.enable": true,
  "volar.takeOverMode.enabled": true,
  "volar.inlayHints.enable": true,
  "volar.codeLens.references": true,
  "volar.codeLens.pugTools": true,
  "volar.codeLens.scriptSetupTools": true,
  "volar.autoCompleteRefs": true,
  "volar.completion.autoImportComponent": true,
  "tsserver.enable": false,
  "typescript.inlayHints.parameterNames.enabled": "none",
  "typescript.inlayHints.parameterTypes.enabled": false,
  "typescript.inlayHints.variableTypes.enabled": false,
  "typescript.showDeprecated": true,
  "typescript.suggest.paths": true,
  "typescript.updateImportsOnFileMove.enable": true,
  "typescript.implementationsCodeLens.enable": false,
  "typescript.format.enabled": true,
  "javascript.inlayHints.parameterNames.enabled": "none",
  "javascript.inlayHints.parameterTypes.enabled": false,
  "javascript.inlayHints.variableTypes.enabled": false
}
